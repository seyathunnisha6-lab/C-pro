#include <stdio.h>
int main() {
    float salary, bonus;
    char gender;
    printf("Enter salary: ");
    scanf("%f", &salary);
    printf("Enter gender (M/F): ");
    scanf(" %c", &gender);

    if (gender == 'M' || gender == 'm')
        bonus = (salary >= 10000) ? 0.05 * salary : 0.07 * salary;
    else
        bonus = (salary >= 10000) ? 0.10 * salary : 0.12 * salary;

    printf("Final Salary = %.2f\n", salary + bonus);
    return 0;
}
